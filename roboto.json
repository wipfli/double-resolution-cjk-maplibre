            "id": "labels",
            "type": "symbol",
            "source": "qrank",
            "source-layer": "qrank",
            "layout": {
                "text-field": [
                    "case",
                    [
                        "==",
                        [
                            "get",
                            "place"
                        ],
                        "country"
                    ],
                    [
                        "get",
                        "name:en"
                    ],
                    [
                        "==",
                        [
                            "get",
                            "place"
                        ],
                        "sea"
                    ],
                    [
                        "get",
                        "name:en"
                    ],
                    [
                        "==",
                        [
                            "get",
                            "place"
                        ],
                        "ocean"
                    ],
                    [
                        "get",
                        "name:en"
                    ],
                    [
                        "has",
                        "ele"
                    ],
                    [
                        "format",
                        [
                            "get",
                            "name"
                        ],
                        {},
                        "\n",
                        {},
                        [
                            "get",
                            "ele"
                        ],
                        {
                            "font-scale": 0.8
                        },
                        "m",
                        {
                            "font-scale": 0.8
                        }
                    ],
                    [
                        "get",
                        "name"
                    ]
                ],
                "text-size": [
                    "interpolate",
                    [
                        "linear"
                    ],
                    [
                        "zoom"
                    ],
                    7,
                    [
                        "*",
