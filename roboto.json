                                        "get",
                                        "@qrank"
                                    ]
                                ],
                                10000000
                            ],
                            19,
                            [
                                ">",
                                [
                                    "to-number",
                                    [
                                        "get",
                                        "@qrank"
                                    ]
                                ],
                                1000000
                            ],
                            18,
                            [
                                ">",
                                [
                                    "to-number",
                                    [
                                        "get",
                                        "@qrank"
                                    ]
                                ],
                                100000
                            ],
                            16,
                            [
                                ">",
                                [
                                    "to-number",
                                    [
                                        "get",
                                        "@qrank"
                                    ]
                                ],
                                10000
                            ],
                            14,
                            11
                        ]
                    ]
                ],
                "text-font": [
