            "paint": {
                "text-halo-width": 1.5,
                "text-halo-color": [
                    "interpolate",
                    [
                        "linear"
                    ],
                    [
                        "zoom"
                    ],
                    13,
                    "rgba(255, 255, 255, 0.7)",
                    14,
                    "rgba(255, 255, 255, 1.0)"
                ]
            }
        },
